<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
					   xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
					   x:Class="Lisa.Dobble.DicePage"
                       x:Name="DiceView"
                       BackgroundColor="#BFBFBF">
    <Frame x:Name="MainGrid" BackgroundColor="#BFBFBF">
        <StackLayout VerticalOptions="Center" HorizontalOptions="Center"  BackgroundColor="#BFBFBF">
            <Image x:Name="DieView" Source="Dice/dice.png" Aspect="Fill"/>
        </StackLayout>
    </Frame>
</ContentPage>